<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="icon" type="image/png" href="/images/favicon.png" />
    <meta
      name="description"
      content="{{ metadata.defaultDescription | safe }}"
    />
    <meta property="og:locale" content="en_US" />
    <meta property="og:type" content="website" />
    <meta property="og:title" content="{{ title | safe }}" />
    <meta
      property="og:site_name"
      content="{{ metadata.defaultDescription | safe }}"
    />
    <meta property="og:url" content="{{ metadata.url | safe }}" />
    <meta
      property="og:image"
      content="{{ metadata.url | safe }}/images/ogimage.png"
    />
    <meta
      property="og:image:secure_url"
      content="{{ metadata.url | safe }}/images/ogimage.png"
    />
    <meta property="og:image:width" content="2048" />
    <meta property="og:image:height" content="1170" />
    <meta
      property="og:description"
      content="{{ metadata.defaultDescription | safe }}"
    />
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:title" content="{{ title | safe }}" />
    <meta name="twitter:site" content="{{ metadata.url | safe }}" />
    <meta
      name="twitter:image"
      content="{{ metadata.url | safe }}/images/ogimage.png"
    />
    <meta name="twitter:creator" content="@tapman90" />
    <meta
      name="twitter:description"
      content="{{ metadata.defaultDescription | safe }}"
    />
    <!-- prettier-ignore -->
    {% block headScripts %}
    <!-- prettier-ignore -->
    {% endblock %}
    <!-- prettier-ignore -->
    {% block styles %}
    <link rel="stylesheet" href="/css/styles.css" />
    <!-- prettier-ignore -->
    {% endblock %}
    <title>{{ title }}</title>
  </head>
  <!-- prettier-ignore -->
  <body>
    <!-- prettier-ignore -->
    {% block body %}
    {{ content | safe }}
    <!-- prettier-ignore -->
    {% endblock %}
  </body>
</html>
